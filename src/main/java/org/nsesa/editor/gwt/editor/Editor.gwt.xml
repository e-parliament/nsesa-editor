<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Editor">
    <inherits name="org.nsesa.editor.gwt.core.Core"/>
    <stylesheet src="css/editor.css"/>

    <source path="client"/>
    <source path="shared"/>
    <public path="public"/>

    <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>
    <inherits name="com.allen_sauer.gwt.log.gwt-log-INFO"/>
    <set-property name="log_WindowLogger" value="DISABLED"/>
    <set-property name="log_DivLogger" value="DISABLED"/>
    <inherits name='com.allen_sauer.gwt.log.gwt-log-RemoteLogger'/>

    <entry-point class="org.nsesa.editor.gwt.editor.client.Editor"/>
</module>