<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style src="PaginationViewImpl.css" field="css"
              type='org.nsesa.editor.gwt.editor.client.ui.amendments.pagination.PaginationViewCss'/>
    <ui:style field="res">
        .label {
            vertical-align: middle;
        }
        .button {
            width: 3em;
        }
    </ui:style>
    <g:HorizontalPanel>
        <g:Button ui:field="first" styleName="{res.button}">&lt;&lt;</g:Button>
        <g:Button ui:field="previous" styleName="{res.button}">&lt;</g:Button>
        <g:Label ui:field="current" styleName="{res.label}"/>
        <g:Button ui:field="next" styleName="{res.button}">&gt;</g:Button>
        <g:Button ui:field="last" styleName="{res.button}">&gt;&gt;</g:Button>
    </g:HorizontalPanel>
</ui:UiBinder>