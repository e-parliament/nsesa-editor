<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style src="AmendmentsHeaderViewImpl.css" field="css"
              type='org.nsesa.editor.gwt.editor.client.ui.amendments.header.AmendmentsHeaderViewCss'/>
    <ui:style field="res">
        .selections {
            height: 100%;
            float: left;
        }
        .actions {
            height: 100%;
            float: left;
        }
    </ui:style>
    <g:HorizontalPanel>
        <g:HTMLPanel>
            <g:ListBox ui:field="selections" styleName="{res.selections}"/>
            <g:ListBox ui:field="actions" styleName="{res.actions}"/>
        </g:HTMLPanel>
    </g:HorizontalPanel>
</ui:UiBinder>