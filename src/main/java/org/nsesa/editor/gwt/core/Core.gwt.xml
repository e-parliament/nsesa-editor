<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Core">
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.user.theme.clean.Clean"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <!-- Guava support -->
    <inherits name="com.google.common.collect.Collect" />
    <!-- GWT Crypto library for SHA-1 checksum -->
    <inherits name="com.googlecode.gwt.crypto.Crypto"/>
    <!-- XPath support -->
    <inherits name="name.pehl.totoe.xml.XML" />
    <!-- JSONPath support -->
    <inherits name="name.pehl.totoe.json.JSON" />
    <!-- Drag & drop -->
    <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>

    <source path="client"/>
    <source path="shared"/>
    <public path="public"/>

</module>